{
  "name": "Rap Battles",
  "tree": {
    "$className": "DataModel",

    "ServerScriptService": {
      "$className": "ServerScriptService",
      "Server": {
        "$path": "src/server"
      },

      "EmojiChatController": {
        "$path": "models/EmojiChatController.rbxm"
      }
    },

    "Chat": {
      "$className": "Chat",

      "BubbleChat": {
        "$path": "assets/BubbleChat.rbxm"
      },

      "ChatLocalization": {
        "$path": "assets/ChatLocalization.rbxm"
      },

      "ChatModules": {
        "$path": "assets/ChatModules.rbxm"
      },

      "ChatScript": {
        "$path": "assets/ChatScript.rbxm"
      },

      "ChatServiceRunner": {
        "$path": "assets/ChatServiceRunner.rbxm"
      },

      "ClientChatModules": {
        "$path": "assets/ClientChatModules.rbxm"
      }
    },

    "ReplicatedStorage": {
      "$className": "ReplicatedStorage",
      "Shared": {
        "$path": "src/shared"
      },
      "Assets": {
        "$path": "assets"
      },
      "Library": {
        "$path": "lib"
      },
      "Packages": {
        "$path": "Packages"
      },
      "DevPackages": {
        "$path": "DevPackages"
      }
    },

    "ReplicatedFirst": {
      "$className": "ReplicatedFirst",
      "Chat": {
        "$path": "includes/Chat"
      }
    },

    "StarterPlayer": {
      "$className": "StarterPlayer",

      "StarterPlayerScripts": {
        "$className": "StarterPlayerScripts",
        "Client": {
          "$path": "src/client"
        },

        "EmojiChatClientController": {
          "$path": "models/EmojiChatClientController.rbxm"
        }
      }
    },

    "Workspace": {
      "$className": "Workspace",
      "Map": {
        "$path": "assets/Map.rbxm"
      }
    },

    "Lighting": {
      "$className": "Lighting",
      "$properties": {
        "GeographicLatitude": 21,
        "ExposureCompensation": 0.05,
        "ClockTime": 12,
        "Technology": "ShadowMap"
      },

      "Bloom" : {
        "$path": "assets/Bloom.rbxm"
      },

      "Dark Skies" : {
        "$path": "assets/Dark Skies.rbxm"
      },

      "Atmosphere": {
        "$path": "assets/Atmosphere.rbxm"
      }
    },

    "HttpService": {
      "$className": "HttpService",
      "$properties": {
        "HttpEnabled": true
      }
    },

    "SoundService": {
      "$className": "SoundService",
      "$properties": {
        "RespectFilteringEnabled": true
      }
    }
  }
}
